#include <stdio.h>
#include <math.h>
#include <locale.h>

#define PI 3.14159265358979323846

int main() 
{
    setlocale(LC_ALL, "RUS");
    double x = 14.26;
    double y = -1.22;
    double z = 3.5e-2;

    double t = (2.0 * cos(x - PI / 6.0)) / (0.5 + sin(y) * sin(y)) *
        (1.0 + (z * z) / (3.0 - (z * z) / 5.0));

    printf("Исходные данные: \n");
    printf("x = %.2f\n", x);
    printf("y = %.2f\n", y);
    printf("z = %.2e\n\n", z);
    printf("Результат вычисления:\n");
    printf("t = %.6f\n", t);

    return 0;
}
